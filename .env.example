# SuperOps API Configuration
SUPEROPS_API_TOKEN=your_api_token_here
SUPEROPS_SUBDOMAIN=your_subdomain
SUPEROPS_DATA_CENTER=us

# ITGlue Export Path (directory containing your export)
EXPORT_PATH=./export-2

# Staging Collection ID (optional - for bulk staging)
# Get this from your SuperOps URL after creating a staging collection
# Example: https://subdomain.superops.ai/#/kb/collection/2540036476765265920
STAGING_COLLECTION_ID=

# Migration Settings
MIGRATION_BATCH_SIZE=10
MIGRATION_DRY_RUN=false
MIGRATION_SKIP_EXISTING=true

# Logging
LOGGING_LEVEL=INFO

# Rate Limiting (requests per minute, max 800)
RATE_LIMIT=750

# Docker Display (for db-viewer, optional)
# DISPLAY=:0